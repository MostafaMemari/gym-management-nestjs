    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:run": "npm run typeorm -- migration:run -d ./src/configs/typeorm.migration.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d ./src/configs/typeorm.migration.ts",
    "migration:generate": "npm run typeorm -- migration:generate -d ./src/configs/typeorm.migration.ts ./src/migrations/$npm_config_name",
    "migration:create": "npm run typeorm -- migration:create ./src/migrations/$npm_config_name"
  },
